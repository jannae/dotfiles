#!/usr/bin/env bash

# Added to prefer homebrew bins over system
export PATH="/usr/local/sbin:$PATH"

# Include user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi

# Add RVM to PATH for scripting
export PATH="$PATH:$HOME/.rvm/bin"

